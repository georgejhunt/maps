#!/bin/bash -x
# define aliases for working with test-page source is inputs for webpack, parcel-bundler

echo Current alias is for: test-page > current_alias
alias put='/bin/cp -f ./main.js ../../osm-source/test-page/main.js;/bin/cp -f ./index.html ../../osm-source/test-page/index.html;/bin/cp -f ./build/* ../../osm-source/test-page/build/'
alias get='/bin/cp -f ../../osm-source/test-page/index.html ./index.html;/bin/cp -f ../../osm-source/test-page/main.js ./main.js'
alias dif='vimdiff ../../osm-source/test-page/main.js ./main.js'
