#!/bin/bash -x
# define aliases for working with test-page source is inputs for webpack, parcel-bundler

echo Current alias is for: test-page > current_alias
alias put='/bin/cp -f ./main.js ../../osm-source/test-page/test.js;/bin/cp -f ./index.html ../../osm-source/test-page/test-index.html;/bin/cp -f ./build/* ../../osm-source/test-page/build/'
alias get='/bin/cp -f ../../osm-source/test-page/test-index.html ./index.html;/bin/cp -f ../../osm-source/test-page/test.js ./main.js'
alias dif='vimdiff ../../osm-source/test-page/test.js ./main.js'
alias pub='scp ./build/main.js ghunt@unleashkids.org:/home/ghunt'
