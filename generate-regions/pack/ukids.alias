#!/bin/bash -x
# define aliases for working with ukids source is inputs for webpack, parcel-bundler

alias put='/bin/cp -f ./main.js ../../osm-source/ukids.main.js;/bin/cp -f ./index.html ../../osm-source/ukids/index.html'
alias get='/bin/cp -f ../../osm-source/ukids/index.html ./index.html;/bin/cp -f ../../osm-source/ukids/main.js ./main.js; cp ../../osm-source/ukids/assets/* .'
alias dif='vimdiff ../../osm-source/ukids/main.js ./main.js'
cp ../../resources/* ./assets

